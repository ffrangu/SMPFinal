            </div>
        </div>
            </form>


    </div>

</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />

    <script>
        $(document).ready(function () {
            Dropzone.autoDiscover = false;
            var myDropzone = new Dropzone("div#kt_dropzone_1", {
                url: "@Url.Action("UploadContract", "Punetori",new {id=Model.Id })", autoProcessQueue: false, addRemoveLinks: true,
                success: function (data) {
                    alert("Ngarkimi i kontrates u krye me sukses!");
                    window.location.href = '@Url.Action("Details","Punetori",new { id=Model.Id})';

                }


            });

            $("#uploadContract").click(function (e) {
                e.preventDefault();
                myDropzone.processQueue();
            });
        });


    </script>
}
