    <div class="d-flex flex-column flex-root">
        <!--begin::Page-->
        <div class="d-flex flex-row flex-column-fluid page">
            <!--begin::Wrapper-->
            <div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
                <!--begin::Header-->
                <div id="kt_header" class="header header-fixed">
                    <!--begin::Container-->
                    <div class="container-fluid">
                        <!--begin::Header Menu Wrapper-->
                        <div class="header-menu-wrapper header-menu-wrapper-left" id="kt_header_menu_wrapper">
                            <!--begin::Logo-->
                            <div class="header-logo mr-10 d-none d-lg-flex">
                                <a href="@Url.Action("Index","Home")">
                                    <img alt="Logo" src="~/assets/media/logos/logo-smp-white.png" class="max-h-100px" />
                                </a>
                            </div>
                            <!--end::Logo-->
                            <!--begin::Header Menu-->
                            <div id="kt_header_menu" class="header-menu header-menu-left header-menu-mobile header-menu-layout-default">
                                <!--begin::Header Nav-->

                                <ul class="menu-nav">
                                    @{
                                        List<MenuItems> menuItems = ViewBag.Menus;

                                        if (menuItems != null)
                                        {
                                            foreach (var item in menuItems)
                                            {
                                                if (item.SubMenu.Count == 0)
                                                {
                                                    <li class="menu-item @(item.Selected ? "menu-item-active":"")" aria-haspopup="true">
                                                        <a href="@Url.Action(item.Action,item.Controller)" class="menu-link">
                                                            <span class="menu-text">@item.Text</span>
                                                            <span class="menu-desc"></span>
                                                        </a>
                                                    </li>
                                                }
                                                else
                                                {
                                                    <li class="menu-item menu-item-submenu menu-item-rel @(item.Selected ? "menu-item-open menu-item-here":"")" data-menu-toggle="click" aria-haspopup="true">
                                                        <a href="javascript:;" class="menu-link menu-toggle">
                                                            <span class="menu-text">@item.Text</span>
                                                            <span class="menu-desc"></span>
                                                            <i class="menu-arrow"></i>
                                                        </a>
                                                        <div class="menu-submenu menu-submenu-classic menu-submenu-left">
                                                            <ul class="menu-subnav">
                                                                @foreach (var sub in item.SubMenu)
                                                                {
                                                                    if (sub.SubMenu.Count > 0)
                                                                    {
                                                                        <li class="menu-item menu-item-submenu menu-item-rel @(sub.Selected ? "menu-item-open menu-item-here":"")" data-menu-toggle="click" aria-haspopup="true">
                                                                            <a href="javascript:;" class="menu-link menu-toggle">
                                                                                <span class="menu-text">@sub.Text</span>
                                                                                <span class="menu-desc"></span>
                                                                                <i class="menu-arrow"></i>
                                                                            </a>
                                                                            <div class="menu-submenu menu-submenu-classic menu-submenu-right">
                                                                                <ul class="menu-subnav">
                                                                                    @foreach (var subsub in sub.SubMenu)
                                                                                    {
                                                                                        <li class="menu-item @(subsub.Selected ? "menu-item-active":"")" aria-haspopup="true">
                                                                                            <a href="@Url.Action(subsub.Action,subsub.Controller)" class="menu-link">
                                                                                                <i class="menu-bullet menu-bullet-line">
                                                                                                    <span></span>
                                                                                                </i>
                                                                                                <span class="menu-text">@subsub.Text</span>
                                                                                            </a>
                                                                                        </li>
                                                                                    }
                                                                                </ul>
                                                                            </div>
                                                                        </li>
                                                                    }
                                                                    else
                                                                    {
                                                                        <li class="menu-item @(sub.Selected ? "menu-item-active":"")" aria-haspopup="true">
                                                                            <a href="@Url.Action(sub.Action,sub.Controller)" class="menu-link">
                                                                                <i class="menu-bullet menu-bullet-line">
                                                                                    <span></span>
                                                                                </i>
                                                                                <span class="menu-text">@sub.Text</span>
                                                                            </a>
                                                                        </li>
                                                                    }
                                                                }

                                                            </ul>
                                                        </div>
                                                    </li>
                                                }
                                            }
                                        }

                                    }
                                </ul>


                                <!--end::Header Nav-->
                            </div>
                            <!--end::Header Menu-->
                        </div>
