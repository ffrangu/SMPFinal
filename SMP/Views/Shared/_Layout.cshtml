    <script type="text/javascript">
        $(document).ready(function () {
            Dropzone.autoDiscover = false;
            $('#searchbox').on('input',function () {

                var value = $('#searchbox').val();


                $.ajax({
                    url: '@Url.Action("Search","Punetori")',
                    type: 'POST',
                    data: { value: value },
                    success: function (data) {
                        debugger;
                        const myNode = document.getElementById("searchresults");
                        if (data.length == 0) {

                            $('.quick-search-wrapper').hide();

                            while (myNode.firstChild) {
                                myNode.removeChild(myNode.lastChild);
                            }

                        }
                        else {
                            $('.quick-search-wrapper').show();
                            while (myNode.firstChild) {
                                myNode.removeChild(myNode.lastChild);
                            }


                            //for (i = 0; i < data.length; i++) {
                            //    $('#searchresults').html('<a id="'+data[i].Id+'"class="dropdown - item" href="">Action</a>');
                            //}

                            $.each(data, function (index, element) {
                                var url = '@Url.Action("Details", "Punetori")?id=' + element.id;
                                $('#searchresults').append($('<a>', {
                                    text: element.emri+" "+ element.mbiemri,
                                    class: "dropdown - item",
                                    href : url
                                }));
                            });






                        }



                    },
                error: function () {
                    alert("error");
                }
            });




            });

            $(".init-select2").select2();
            $('.datepicker').datepicker({
                format: "dd/mm/yyyy",
                autoclose: true,
            });
            $('#userTable').DataTable({
                "bSort": true,
                "bLengthChange": false
            });

            $('#data-table').DataTable({
                "bSort": true,
                "bLengthChange": false
            });

            $('#data-table-nosort').DataTable({
                "bSort": false,
                "bLengthChange": false,
                "searching": true,              // Searchbox
                "dom": '<"top"f>rt<"bottom"lp><"clear">', // Positions table elements
                "language": {
                    "search": "_INPUT_",            // Removes the 'Search' field label
                    "searchPlaceholder": "Search"   // Placeholder for the search box
                },
                "search": {
                    "addClass": 'form-control input-lg col-xs-12'
                }
            });
        });
    </script>



</body>
</html>
